<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/settings"
    android:layout_width="match_parent"
    android:layout_height="fill_parent"
    android:background="@drawable/main_bg_rest2"
    android:orientation="vertical"
    android:padding="10dp" >

    <ImageView
        android:id="@+id/bg_about"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="40dp"
        android:layout_marginRight="40dp"
        android:layout_marginTop="10dp"
        android:contentDescription="@string/logo"
        android:src="@drawable/logo" />

    <TextView
        android:id="@+id/resetProgressText"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:gravity="center_horizontal"
        android:text="Project of Team #13"
        android:textColor="#000000"
        android:textSize="33sp" />

    <TextView
        android:id="@+id/tm1"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:gravity="center_horizontal"
        android:padding="5dp"
        android:text="Ben Brown"
        android:textColor="#000000"
        android:textSize="30sp" />

    <TextView
        android:id="@+id/tm2"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:gravity="center_horizontal"
        android:padding="5dp"
        android:text="Callan White"
        android:textColor="#000000"
        android:textSize="30sp" />

    <TextView
        android:id="@+id/tm3"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:gravity="center_horizontal"
        android:padding="5dp"
        android:text="Xiaoquan Ge"
        android:textColor="#000000"
        android:textSize="30sp" />

    <TextView
        android:id="@+id/tm4"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:gravity="center_horizontal"
        android:padding="5dp"
        android:text="David Orizu"
        android:textColor="#000000"
        android:textSize="30sp" />

    <TextView
        android:id="@+id/tm5"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:gravity="center_horizontal"
        android:padding="5dp"
        android:text="Mantas Sutula"
        android:textColor="#000000"
        android:textSize="30sp" />

    <TextView
        android:id="@+id/tm6"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:gravity="center_horizontal"
        android:padding="5dp"
        android:text="James Imrie"
        android:textColor="#000000"
        android:textSize="30sp" />

    <TextView
        android:id="@+id/tm7"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:gravity="center_vertical|center_horizontal"
        android:padding="5dp"
        android:text="Viktor Movcan"
        android:textColor="#000000"
        android:textSize="30sp" />

</LinearLayout>